<?xml version="1.0" encoding="utf-8"?>
<music.dexterous.com.dexterousmusic.customeviews.slidinguppannel.SlidingUpPanelLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:sothree="http://schemas.android.com/apk/res-auto"
    android:id="@+id/sliding_layout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:gravity="bottom"
    sothree:umanoPanelHeight="@dimen/home_activity_bottom_bar"
    sothree:umanoShadowHeight="4dp">

    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/home_activity_backgroud_image_view"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:scaleType="centerCrop" />

        <FrameLayout
            android:id="@+id/rootHomeContainer"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </FrameLayout>

    <FrameLayout
        android:id="@+id/rootHomeContainerBottom"
        android:layout_width="match_parent"
        android:layout_height="match_parent" />

</music.dexterous.com.dexterousmusic.customeviews.slidinguppannel.SlidingUpPanelLayout>
